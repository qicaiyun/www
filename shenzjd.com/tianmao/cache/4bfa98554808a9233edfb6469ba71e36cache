                        var paramBoole =true;
                        for(var a = 0 ;a < coutData[s].is_param.split(',').length ; a ++){
                            var paramKey = coutData[s].is_param.split(',')[a]+"=";
                            if(coutData[s].is_param.split(',')[a] != ''){
                                if( window.location.href.split(paramKey).length<2){
                                    paramBoole = false;
