    $(function () {
        $(".buy-wrapper .normal-btn, .detail-wrapper .img > img.normal-img").click(function () {
            var _url = $(this).data("href");
            _url = typeof(_url) == "undefined"?$(this).attr("href"):_url;

            var ua = navigator.userAgent.toLowerCase();
            if (ua.match(/MicroMessenger/i) == 'micromessenger') {
                if (/iphone|ipad|ipod/.test(ua)) {
                    var img_url = "wechat-ios";
                } else if (/android|adr|linux/.test(ua)) {
                    var img_url = "wechat-android";
                }
                layer.open({
                    type: 1,
                    title: false,
                    closeBtn: 0,
                    shadeClose: true,
                    shade: 0.6,
                    offset: "125px",
                    skin: 'mask-tips',
                    content: '<img class="wechat-bg" src="//cms6.qingtaoke.com/images/' + img_url + '.png" /><img class="wechat-arrow" src="//cms6.qingtaoke.com/images/wechat-arrow.png" />'
                });
                return false;
            } else {
                window.location.href = _url;
            }
        });
    })
</script>

