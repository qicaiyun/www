<style>
    html body .layui-layer {
        background-color: transparent;
        box-shadow: none;
    }

    .my_layer .double-img {
        width: 100%;
    }
    .closeS11 {
        position: absolute;
        width: 30px;
        height: 30px;
        z-index: 10000;
        top: -1px;
        right: 0;
    }
    .closeS11 img{
        width: 30px;
