<div class="toTop" style="display: none" onclick=" $('body,html').animate({scrollTop:0},1000);"><span>回到顶部</span></div>
<script type="text/javascript">
    // 设置为主页
    function SetHome(obj, vrl) {
        try {
            obj.style.behavior = 'url(#default#homepage)';
            obj.setHomePage(vrl);
        }
        catch (e) {
            if (window.netscape) {
                try {
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                }
                catch (e) {
                    alert("此操作被浏览器拒绝！\n请在浏览器地址栏输入“about:config”并回车\n然后将 [signed.applets.codebase_principal_support]的值设置为'true',双击即可。");
                }
                var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
                prefs.setCharPref('browser.startup.homepage', vrl);
            } else {
                alert("您的浏览器不支持，请按照下面步骤操作：1.打开浏览器设置。2.点击设置网页。3.输入：" + vrl + "点击确定。");
            }
        }
    }
    // 加入收藏 兼容360和IE6
    function shoucang(sTitle, sURL) {
        try {
            window.external.addFavorite(sURL, sTitle);
        }
        catch (e) {
            try {
                window.sidebar.addPanel(sTitle, sURL, "");
            }
            catch (e) {
                alert("加入收藏失败，请使用Ctrl+D进行添加");
            }
        }
    }
    // toTop&fixedHead
    function showElem() {
        if ($(window).scrollTop() > 168) {
            $(".toTop").stop().fadeIn(200);
            $(".fixedHead-box").stop().slideDown(200);
        } else {
            $(".toTop").stop().fadeOut(200);
            $(".fixedHead-box").stop().slideUp(200);
        }
    }
    $(function () {
        showElem();
        $(window).scroll(function () {
            showElem()
        });

        $('#navFeedback').on('click', function () {
            layer.open({
                type: 1,
                //skin:'layui-layer-rim',
                area: [600, 400],
                title: false,
                closeBtn: 1,
                shadeClose: true,
                //btn:['提交'],
                content: $('#feedback')
            });
        });
        $('#feedbackSubmit').on('click', function () {
            var fContent = $('#feedbackContent').val();
            var fContact = $('#feedbackContact').val();
            if (fContent.trim() == '') {
                layer.msg('请留下您的建议', {time: 1500});
                return;
            }
            if (fContact.trim() == '') {
                layer.msg('请留下您的联系方式', {time: 1500});
                return;
            }
            var feedbackSuccess = function (ret) {
                layer.closeAll();
                $('#feedbackContent').val('');
                $('#feedbackContact').val('');
                layer.open({
                    type: 1,
                    area: [600, 400],
                    title: false,
                    closeBtn: 0,
                    shadeClose: true,
                    content: $('#feedbackSuccess')
                });
            }
            $.ajax('index.php?r=Feedback/add', {
                type: 'post',
                dataType: 'json',
                data: {
                    "content": fContent.trim(),
                    "contact": fContact.trim(),
                    // 处理当uid为空时，js不报错。
                    "uid":36050                },
                error: function (jqXHR, textStatus, errorThrown) {
                    return layer.msg(errorThrown);
                },
                success: feedbackSuccess
            });
        });
        $('#feedbackClose').on('click', function () {
            layer.closeAll();
        });
    });

</script>
