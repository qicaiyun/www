<style>
    #load-more {
        overflow: hidden;
    }

    .goods-list {
        overflow: hidden;
    }

    .cat-item {
        padding: 15px 0;
    }

    .cat-item a {
        margin: 0;
    }

    #new-left-menu {
        position: fixed;
        height: 100%;
        width: 100%;
        left: -100%;
        top: 0;
        overflow-y: scroll;
    }

    #new-left-menu .mask {
        width: 100%;
        height: 100%;

    }

    #new-left-menu .menu-content {
        position: absolute;
        top: 0;
        left: 0;
        width: 80%;
        height: 100%;
        background-color: #fff;
        overflow-y: auto;
    }

    #menu-mask {
        background-color: rgba(0, 0, 0, 0.4);
    }

    .main-cat {
        margin: 0;
    }

    html body .ui-loader {
        display: none;
