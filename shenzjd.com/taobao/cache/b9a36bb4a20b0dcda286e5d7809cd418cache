                var fkType = $(this).attr('data-cnzz-type');
                var tPaht = "/taobao/index.php";
                var reqUrl = "/taobao/?r=l&cid=0&kw=&s=t&min=0&max=0&seckill=0&jipin=0&tmall=0&page=5&pv=6";
