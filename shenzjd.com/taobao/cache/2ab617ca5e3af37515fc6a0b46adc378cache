                    data: {r: 'site/cnzz', fkId: fkId, req: reqUrl, cnzzType: fkType},
                    dataType: 'json',
                    type: 'post',
                    error: function (xhr, type, errorThrown) {

                    },
                    success: function (result, status, xhr) {

                    }
                });
            });
